mixin m-head
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    title Web Project
    meta(name='description' content='Web Project')
    meta(name='author' content='ASTEQ')
    meta(property='og:title' content='Web Project')
    meta(property='og:type' content='website')
    meta(property='og:url' content='write your link')
    meta(property='og:description' content='Web Project')
    meta(property='og:image' content='write your path')
    link(rel='icon' href='write your path')
    link(rel='icon' href='write your path' type='image/svg+xml')
    link(rel='apple-touch-icon' href='write your path')

mixin m-header 
  header.m-header 
    .m-header__container-wrap 
      nav.m-header__nav
        .m-header__logo 
          include ../../assets/icons/CookToo-Logo.svg
        ul.m-header__list
          li
            +b-textLink('b-textLink --white --s-18') О нас
          li 
            +b-textLink('b-textLink --white --s-18') Создать набор
          li 
            +b-textLink('b-textLink --white --s-18') Оплата и доставка
          li 
            +b-textLink('b-textLink --white --s-18') Как мы готовим?
          li 
            +b-textLink('b-textLink --white --s-18') Контакты
      .m-header__actionButton 
        +b-textLink('b-textLink --white --s-18') +7 928 246 50 33
        +b-button('b-roundButton') Заказать
      +b-IconButton('b-menuButton --white')
        include ../../assets/icons/Actions/Menu.svg




































































    