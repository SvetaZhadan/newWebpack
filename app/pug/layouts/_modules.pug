mixin m-headerMenu
  .m-headerMenu
    nav.m-headerMenu__nav
      a(href="#") 
        +b-textLink('b-textLink --black --s-20') О нас
      a(href="#") 
        +b-textLink('b-textLink --black --s-20') Создать набор
      a(href="#") 
        +b-textLink('b-textLink --black --s-20') Оплата и доставка
      a(href="#") 
        +b-textLink('b-textLink --black --s-20') Как мы готовим?
      a(href="#footer") 
        +b-textLink('b-textLink --black --s-20') Контакты
    .m-headerMenu__socials
      +b-IconButton('b-socialLink --twitter --outline')
        include ../../assets/icons/Socials/Twitter.svg
      +b-IconButton('b-socialLink --vk --outline')
        include ../../assets/icons/Socials/vk.svg
      +b-IconButton('b-socialLink --facebook --outline')
        include ../../assets/icons/Socials/facebook.svg
    .m-headerMenu__contacts 
      +b-textLink('b-textLink --black --s-20') +7 928 246 50 33
      .m-headerMenu__address г. Краснодар, ул. Ставропольская, д. 123


mixin m-header 
  header.m-header 
    .m-header__container 
      .m-header__left
        a(href="#").m-header__logo.--white
          include ../../assets/icons/CookToo-Logo-alt.svg
        nav.m-header__list
            a(href="#") 
              +b-textLink('b-textLink --s-18') О нас
            a(href="#") 
              +b-textLink('b-textLink --s-18') Создать набор 
            a(href="#") 
              +b-textLink('b-textLink --s-18') Оплата и доставка
            a(href="#") 
              +b-textLink('b-textLink --s-18') Как мы готовим? 
            a(href="#footer")  
              +b-textLink('b-textLink --s-18') Контакты
      .m-header__right 
        +b-textLink('b-textLink --white --s-18') +7 928 246 50 33
        +b-button('b-roundButton') Заказать
      +b-burger
    +m-headerMenu

